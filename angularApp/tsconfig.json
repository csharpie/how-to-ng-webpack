{
  "compilerOptions": {
    "outDir": "./wwwroot/out-tsc",
    "baseUrl": "angularApp",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es5",
    "types": ["node"],
    "typeRoots": [
      "../node_modules/@types"
    ],
    "lib": ["es5", "es6", "dom"]
  },
  "exclude": [
    "node_modules/*"
  ]
}
